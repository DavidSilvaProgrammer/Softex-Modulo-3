class Cubo:
    '''Classe para calcular o cubo de um número''' #docstring é quando tem uma descrição longa com 3 aspas simples
    def __init__(self,valor): #método construtor da classs
        self.x=valor
        print('Objeto criado!')
    def calcula_cubo(self): #método de uso geral
        cubo=self.x*self.x*self.x
        return 'Cubo calculado: ' + str(cubo) 
        
teste=Cubo(6)
c=teste.calcula_cubo()
print(c)
