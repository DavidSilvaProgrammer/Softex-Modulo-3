class Gato:
    '''Classe para trabalhar com gatos'''
    def __init__(self,nome):
        '''MÃ©todo construtor: Inicializa o objeto capturando o nome do animal'''
        self.nome=nome
        print('Seu gatinho se chama',self.nome)
        
nome_gato=input('Digite o nome de seu gato: ')
g1=Gato(nome_gato)
